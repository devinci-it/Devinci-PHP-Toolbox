{
    "name": "devinci/dev-core",
    "description": "Dev Core Library",
    "require": {
        "illuminate/database": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Devinci\\DevCore\\ComposerUtils\\": "ComposerUtils",
            "Devinci\\DevCore\\Config\\": "Config",
            "Devinci\\DevCore\\EloquentUtils\\": "EloquentUtils",
            "Devinci\\DevCore\\": ""
        }
    },
    "extra": {
        "devinci-script-params": {
            "vendorName": "devinci",
            "baseLibraryPath": "__DIR__ . '/../../src'",
            "composerJsonPath": "__DIR__ . '/../../composer.json'"
        }
    }
}
